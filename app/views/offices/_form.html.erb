<%= simple_form_for(@office) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :city %>
    <%= f.input :country %>
    <%= f.input :address %>
    <%= f.input :email %>
    <%= f.input :website %>
    <%= f.input :twitter %>
    <%= f.input :pinterest %>
    <%= f.input :mailchimp %>
    <%= f.input :mixcloud %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
