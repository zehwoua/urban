<h1>Listing offices</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Country</th>
      <th>Address</th>
      <th>Email</th>
      <th>Website</th>
      <th>Twitter</th>
      <th>Pinterest</th>
      <th>Mailchimp</th>
      <th>Mixcloud</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @offices.each do |office| %>
      <tr>
        <td><%= office.city %></td>
        <td><%= office.country %></td>
        <td><%= office.address %></td>
        <td><%= office.email %></td>
        <td><%= office.website %></td>
        <td><%= office.twitter %></td>
        <td><%= office.pinterest %></td>
        <td><%= office.mailchimp %></td>
        <td><%= office.mixcloud %></td>
        <td><%= link_to 'Show', office %></td>
        <td><%= link_to 'Edit', edit_office_path(office) %></td>
        <td><%= link_to 'Destroy', office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Office', new_office_path %>
